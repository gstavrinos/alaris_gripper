            <inertia ixx="0.003881243" ixy="0.0" ixz="0.0"
                     iyy="0.000498940" iyz="0.0"
                     izz="0.003879257" />
        </inertial>
    </link>

    <joint name="${prefix}_finger_${finger_id}_phalanx2_top_shaft2_joint" type="fixed">
