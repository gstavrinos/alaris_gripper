            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://alaris_gripper_description/meshes/Gripper_Assembly - phalanx_2.STL" scale="0.001 0.001 0.001"/>
