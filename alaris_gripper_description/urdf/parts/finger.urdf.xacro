            </geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://alaris_gripper_description/meshes/Gripper_Assembly - shaft_2.STL" scale="0.001 0.001 0.001"/>
